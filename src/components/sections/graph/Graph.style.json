[
    {
      "selector": "node",
      "style": {
        "background-color": "gray",
        "label": "data(id)",
        "height": "15px",
        "width": "15px"
      }
    },
    {
      "selector": "node[?isMutated]",
      "style": {
        "shape": "star",
        "backgroundColor": "orange"
      }
    },
    {
      "selector": ".isGrouped",
      "style": {
        "background-color": "green"
      }
    },
    {
      "selector": "edge",
      "style": {
        "width": 1,
        "curve-style": "bezier",
        "arrow-scale": 2
      }
    },
    {
      "selector": "[function = 'inhibits']",
      "style": {
        "lineColor": "red",
        "target-arrow-color": "red",
        "target-arrow-shape": "diamond"
      }
    },
    {
      "selector": "[function = 'activates']",
      "style": {
        "lineColor": "green",
        "target-arrow-color": "green",
        "target-arrow-shape": "triangle-backcurve"
      }
    },
    {
      "selector": "[function = 'suppress']",
      "style": {
        "lineColor": "purple",
        "target-arrow-color": "purple",
        "target-arrow-shape": "tee"
      }
    },
    {
      "selector": "edge[^function]",
      "style": {
        "lineColor": "blue"
      }
    }
  ]