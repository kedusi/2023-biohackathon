[
    {
      "selector": "node",
      "style": {
        "background-color": "blue",
        "label": "data(id)",
        "height": "15px",
        "width": "15px"
      }
    },
    {
      "selector": ".isMutated",
      "style": {
        "shape": "star",
        "backgroundColor": "orange"
      }
    },
    {
      "selector": ".isGrouped",
      "style": {
        "background-color": "green"
      }
    },
    {
      "selector": "edge",
      "style": {
        "width": 1,
        "curve-style": "bezier",
        "arrow-scale": 1
      }
    },
    {
      "selector": "[function *= 'Low (score <0.5)']",
      "style": {
        "lineColor": "green"
      }
    },
    {
      "selector": "[function = 'Medium (0.5 <= score < 0.8)']",
      "style": {
        "lineColor": "red"
      }
    },
    {
      "selector": "[function = 'high (0.8 <= score < 1)']",
      "style": {
        "lineColor": "black"
      }
    }
  ]